{
  "manifest_version": 2,
  "name": "Mute Notifications",
  "description": "Disable all desktop notifications with one click.",
  "version": "0.3",
  "icons": {
    "128": "logo-128.png",
    "48": "logo-48.png"
  },
  "permissions": [
    "contentSettings",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon-on-19.png",
      "38": "icon-on-38.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
